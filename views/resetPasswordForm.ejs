<% include header %>

<div class="resetPasswordConfirm">
    <form action= "/users/resetPasswordConfirm" method="post">
        <input type="text" name="token" value= <%= token %> >
        <input type="text" name="newPassword" placeholder="Enter New Password Here...">
        <input type="text" name="newPasswordConfirm" placeholder="Re-Enter New Password Here...">
        <input type="submit" value="Confirm!">
    </form>
</div>

<% include footer %>